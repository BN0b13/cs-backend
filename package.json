{
  "name": "cosmicstrains_backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "start": "npm run migrate && nodemon app.js",
    "up": "docker-compose -f ./docker-compose.yml -p cosmic_strains up -d",
    "down": "docker-compose -f ./docker-compose.yml -p cosmic_strains down",
    "destroy": "docker-compose -f ./docker-compose.yml -p cosmic_strains down -v --remove-orphans",
    "migrate": "sequelize-cli db:migrate",
    "migrate:down": "sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all --seeders-path seeders/prod",
    "seed:dev": "npx sequelize-cli db:seed:all --seeders-path seeders/dev",
    "redis": "docker-compose -f ./redis.docker-compose.yml -p order_queue up -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BN0b13/cs-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BN0b13/cs-backend/issues"
  },
  "homepage": "https://github.com/BN0b13/cs-backend#readme",
  "dependencies": {
    "@sendgrid/mail": "^8.1.0",
    "@square/web-sdk": "^2.0.1",
    "bcrypt": "^5.1.0",
    "bull": "^4.11.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.3.2",
    "path": "^0.12.7",
    "pg": "^8.10.0",
    "sequelize": "^6.30.0",
    "sharp": "^0.33.2",
    "socket.io": "^4.7.5",
    "square": "^25.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "sequelize-cli": "^6.6.0"
  }
}
